{
    "name": "asm/eprel-api-client",
    "description": "Eprel API Client",
    "type": "library",
    "require": {
        "php": "^8.4",
        "guzzlehttp/guzzle": "^7.8",
        "symfony/console": "^7.0",
        "symfony/yaml": "^7.0",
        "psr/cache": "^3.0",
        "psr/log": "^3.0",
        "psr/http-client": "^1.0",
        "psr/http-factory": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.0",
        "vimeo/psalm": "^6.0",
        "phpstan/phpstan": "^2.0",
        "squizlabs/php_codesniffer": "^3.9"
    },
    "autoload": {
        "psr-4": {
            "Asm\\EprelApiClient\\": "Asm/EprelApiClient/"
        },
        "exclude-from-classmap": [
            "Asm/EprelApiClient/Tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Asm\\EprelApiClient\\Tests\\": "Asm/EprelApiClient/Tests/"
        }
    }
}
